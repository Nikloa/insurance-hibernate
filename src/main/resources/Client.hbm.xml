<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.vironit.app.entities.Client" table="clients" schema="public" catalog="insurance_service">
        <id name="id" column="id"><generator class="increment"/></id>
        <property name="nickname" column="nickname"/>
        <property name="email" column="email"/>
        <property name="password" column="password"/>
        <property name="phone" column="phone"/>
        <property name="photo" column="photo"/>
        <property name="balance" column="balance"/>
        <property name="rating" column="rating"/>
    </class>
</hibernate-mapping>