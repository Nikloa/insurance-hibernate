<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.vironit.app.entities.Passport" table="passports" schema="public" catalog="insurance_service">
        <id name="id" column="id"><generator class="increment"/></id>
        <property name="clientId" column="client_id"/>
        <property name="firstName" column="first_name"/>
        <property name="lastName" column="last_name"/>
        <property name="birthDate" column="birth_date"/>
        <property name="passportNumber" column="passport_number"/>
        <property name="identificationNumber" column="identification_number"/>
        <property name="issueDate" column="issue_date"/>
        <property name="issuingAuthority" column="issuing_authority"/>
        <property name="passportPhoto" column="passport_photo"/>
        <property name="confirmation" column="confirmation"/>
    </class>
</hibernate-mapping>